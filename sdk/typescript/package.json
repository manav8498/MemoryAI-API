{
  "name": "memory-ai-ts-sdk",
  "version": "1.0.0",
  "description": "Official TypeScript/JavaScript SDK for AI Memory API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "memory",
    "llm",
    "vector",
    "search",
    "rag",
    "knowledge-base",
    "embeddings",
    "reasoning"
  ],
  "author": "AI Memory Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/memory-ai/memory-ai-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/memory-ai/memory-ai-typescript/issues"
  },
  "homepage": "https://memory-ai.com",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
